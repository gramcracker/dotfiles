! -*- coding: utf-8 -*-
! swap keys {capslock, ctrl}
! swap keys {shift, windows key}

remove mod4 = Super_L
remove Shift = Shift_L
remove Lock = Caps_Lock

keysym Shift_L = Super_L
keysym Super_L = Shift_L
keysym Caps_Lock = Control_L

add Control = Control_L
add Shift = Shift_L
add mod4 = Super_L

